{
  "name": "sage",
  "version": "1.0.0",
  "description": "evergreen-ai-service",
  "main": "dist/main.js",
  "type": "module",
  "packageManager": "pnpm@10.27.0",
  "scripts": {
    "build": "pnpm typecheck && vite build",
    "chat-with-staging": "vite-node src/mastra/tests/scripts/chat-with-staging.ts",
    "clean": "rm -rf dist bin node_modules",
    "codegen": "graphql-codegen-esm --config graphql.config.ts",
    "deploy:staging": "NODE_ENV=staging dotenv-flow -- sh scripts/deploy.sh",
    "dev-run": "vite-node --watch src/main.ts",
    "dev:prod": "VERSION=$(git rev-parse --short HEAD 2>/dev/null || echo 'unknown') NODE_ENV=production dotenv-flow pnpm dev-run",
    "dev:staging": "VERSION=$(git rev-parse --short HEAD 2>/dev/null || echo 'unknown') NODE_ENV=staging dotenv-flow pnpm dev-run",
    "dev": "VERSION=$(git rev-parse --short HEAD 2>/dev/null || echo 'unknown') NODE_ENV=development pnpm dev-run",
    "e2e": "vitest --project e2e",
    "eslint:fix": "pnpm eslint:strict --fix",
    "eslint:staged": "STRICT=1 eslint",
    "eslint:strict": "STRICT=1 eslint .",
    "format": "prettier --write src/**/*.ts \"**/*.md\" \"*.md\"",
    "format:check": "prettier --check src/**/*.ts \"**/*.md\" \"*.md\"",
    "generate-cursor-client": "openapi-ts",
    "run-analyzer": "vite-node src/mastra/tests/scripts/run-log-analyzer.ts",
    "typecheck": "tsc",
    "test-thinking-agent": "vite-node src/mastra/tests/scripts/test-thinking-agent.ts",
    "mastra:symlink-env": "mkdir -p src/mastra/public && for f in .env .env.*; do [ -f \"$f\" ] && ln -sf \"../../../$f\" \"src/mastra/public/$f\"; done",
    "mastra:dev": "mastra dev",
    "mastra:build": "mastra build",
    "mastra:start": "node --import=./.mastra/output/instrumentation.mjs .mastra/output/index.mjs",
    "mastra:full": "pnpm mastra:build && pnpm mastra:start",
    "prepare": "husky",
    "eval": "BRAINTRUST_EVAL=true npx braintrust eval --external-packages @mastra/core @mastra/braintrust @mastra/observability @mastra/memory @mastra/mongodb --no-send-logs",
    "eval:send_to_braintrust": "BRAINTRUST_EVAL=true npx braintrust eval --external-packages @mastra/core @mastra/braintrust @mastra/observability @mastra/memory @mastra/mongodb --no-progress-bars",
    "load-dataset-into-braintrust": "vite-node src/evals/scripts/braintrust-dataset-loader.ts",
    "pending-commits:json": "bash scripts/check-pending-commits.sh --json",
    "pending-commits": "bash scripts/check-pending-commits.sh",
    "poll-cursor-status:prod": "NODE_ENV=production dotenv-flow pnpm poll-cursor-status",
    "poll-cursor-status:staging": "NODE_ENV=staging dotenv-flow pnpm poll-cursor-status",
    "poll-cursor-status": "vite-node src/services/cursor/jobs/pollCursorAgentStatusJob.ts",
    "poll-jira:prod": "NODE_ENV=production dotenv-flow pnpm poll-jira",
    "poll-jira:staging": "NODE_ENV=staging dotenv-flow pnpm poll-jira",
    "poll-jira": "vite-node src/services/jira/jobs/pollJiraTicketsJob.ts",
    "start": "NODE_ENV=production node dist/main.js",
    "test:coverage": "pnpm test --coverage",
    "test:run": "pnpm test run",
    "test:ui": "pnpm test --ui",
    "test": "vitest --project unit"
  },
  "dependencies": {
    "@ai-sdk/azure": "^2.0.89",
    "@ai-sdk/openai": "^2.0.71",
    "@ai-sdk/provider": "^3.0.8",
    "@mastra/ai-sdk": "^1.0.3",
    "@mastra/braintrust": "^1.0.2",
    "@mastra/core": "^1.2.0",
    "@mastra/loggers": "^1.0.1",
    "@mastra/memory": "^1.1.0",
    "@mastra/mongodb": "^1.2.0",
    "@mastra/observability": "^1.2.0",
    "@mastra/rag": "^2.1.0",
    "@mastra/server": "^1.2.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.69.0",
    "@opentelemetry/context-async-hooks": "^2.5.0",
    "@opentelemetry/core": "^2.5.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.211.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.211.0",
    "@opentelemetry/sdk-trace-base": "^2.2.0",
    "@opentelemetry/semantic-conventions": "^1.39.0",
    "@opentelemetry/winston-transport": "^0.21.0",
    "@sentry/node": "10.38.0",
    "@sentry/vite-plugin": "^4.6.0",
    "ai": "^5.0.129",
    "braintrust": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv-flow": "^4.1.0",
    "express": "^4.18.2",
    "express-list-endpoints": "^7.1.1",
    "express-winston": "^4.2.0",
    "gpt-tokenizer": "^3.0.1",
    "gql": "^1.1.2",
    "graphql": "^16.11.0",
    "graphql-tag": "^2.12.6",
    "jira.js": "^5.3.0",
    "mongodb": "7.1.0",
    "query-string": "^9.3.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "winston-transport": "^4.9.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@eslint/js": "^9.30.1",
    "@graphql-codegen/cli": "^5.0.7",
    "@hey-api/client-fetch": "^0.13.1",
    "@hey-api/openapi-ts": "^0.91.1",
    "@opentelemetry/propagation-utils": "^0.31.14",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/express-winston": "^4.0.2",
    "@types/node": "^20.5.0",
    "@types/supertest": "^6.0.3",
    "@types/winston": "^2.4.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoevals": "^0.0.131",
    "csv-parse": "^6.1.0",
    "dotenv-flow-cli": "^1.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^61.5.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-sort-destructure-keys": "^2.0.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "junit-report-builder": "^5.1.1",
    "lint-staged": "^16.2.3",
    "mastra": "^1.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.42.0",
    "vite": "^7.2.7",
    "vite-node": "^3.2.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "author": "",
  "license": "ISC"
}
