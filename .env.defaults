PORT=8080

# Evergreen
EVERGREEN_API_USER=sage
EVERGREEN_API_KEY=mysecret
USER_NAME=regular

# DB
DB_NAME=sage


# Logging
LOG_LEVEL=debug

# Secrets
AZURE_OPENAI_API_KEY=
AZURE_OPENAI_ENDPOINT=
OTEL_COLLECTOR_URL=

# Encryption
# ENCRYPTION_KEY: 32-byte hex string (64 characters) for AES-256 encryption
# Generate with: openssl rand -hex 32
ENCRYPTION_KEY=

# Sentry Error Tracking
# Set SENTRY_ENABLED=true and provide a DSN to enable error tracking
SENTRY_ENABLED=false
SENTRY_DSN=
SENTRY_ENVIRONMENT=development
SENTRY_SAMPLE_RATE=1.0
SENTRY_TRACES_SAMPLE_RATE=1.0
SENTRY_DEBUG=false
SENTRY_ATTACH_STACKTRACE=true
SENTRY_CAPTURE_CONSOLE=false
